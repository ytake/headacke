{
  "name": "ytake/hh-container",
  "description": "simple light weight service location / dependency injection container",
  "minimum-stability": "stable",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "hack",
    "hhvm",
    "container"
  ],
  "require": {
    "hhvm": ">=3.28",
    "hhvm/hhvm-autoload": "^1.6",
    "hhvm/hsl": "^3.28",
    "ytake/psr-container-hhi": "^1.0"
  },
  "require-dev": {
    "hhvm/hacktest": "^1.0",
    "facebook/fbexpect": "^2.1.3",
    "hhvm/hhast": ">=3.28.0"
  },
  "autoload": {
    "psr-4": {
      "Ytake\\HHContainer\\": "src/"
    }
  },
  "autoload-dev": {
  "classmap": [
      "tests/"
    ] 
  }
}
